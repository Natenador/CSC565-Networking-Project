package messages;

import java.io.Serializable;

public class RequestMessage implements Serializable{

	public static final int REQUEST_COMMAND_LIST = 0;
	/**
	 * 
	 */
	private static final long serialVersionUID = 8420419057923672407L;
	
	private int id;
	private int command;
	private int viewRequested;
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public int getCommand() {
		return command;
	}
	public void setCommand(int command) {
		this.command = command;
	}
	public int getViewRequested() {
		return viewRequested;
	}
	public void setViewRequested(int viewRequested) {
		this.viewRequested = viewRequested;
	}

}

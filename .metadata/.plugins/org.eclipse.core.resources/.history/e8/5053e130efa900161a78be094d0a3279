package messages;

import java.io.Serializable;

import test.Device;

public abstract class HouseHubMessage implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = -7238868535000515345L;
	
	private int id;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

}
